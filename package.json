{
  "name": "logdna-s3",
  "version": "1.1.0",
  "description": "Lambda Function to Stream Logs from AWS S3 to LogDNA",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": [
      "node_modules/",
      "coverage/"
    ],
    "extends": [
      "logdna"
    ],
    "parserOptions": {
      "ecmaVersion": 2019
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "agentkeepalive": "^4.0.2",
    "async": "^2.6.2",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.590.0",
    "eslint": "^7.4.0",
    "eslint-config-logdna": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sensible": "^2.2.0"
  },
  "keywords": [
    "lambda",
    "logdna",
    "aws",
    "s3"
  ],
  "author": "LogDNA",
  "license": "MIT"
}
